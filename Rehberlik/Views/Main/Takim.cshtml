@model IEnumerable<Rehberlik.Models.SqlDat.Team>
@{
    ViewData["Title"] = "Takim";
    Layout = "~/Views/_VitrinLayout.cshtml";
}



<div class="section breadcrumb-section">
    <span class="shape shape-one layer" data-speed="1">
        <img src="~/3/assets/images/slider/shape-2.png" alt="Shape">
    </span>
    <span class="shape shape-two layer" data-speed="2">
        <img src="~/3/assets/images/slider/shape-5.png" alt="Shape">
    </span>
    <span class="shape shape-three wow pulse" data-wow-duration="1s" data-wow-delay=".1s">
        <img src="~/3/assets/images/slider/shape-7.png" alt="Shape">
    </span>
    <span class="shape shape-four wow fadeInUp" data-wow-duration="1s" data-wow-delay=".1s">
        <img src="~/3/assets/images/slider/shape-9.png" alt="Shape">
    </span>
    <span class="shape shape-five wow fadeInUp" data-wow-duration="1.5s" data-wow-delay=".1s">
        <img src="~/3/assets/images/slider/shape-10.png" alt="Shape">
    </span>
    <div class="container">
        <div class="row">
            <div class="col-12 align-items-center text-center">
                <div class="breadcrumb-wrapper">
                    <h1 class="title">Ekibimiz</h1>
                    <p class="text mb-0">Kaliteli Hizmet İçin Buradayız. </p>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Breadcrumb Section End -->
<!-- Team Section Start -->
<div class="team-section all-members section-margin">
    <div class="container">
        <div class="row mb-n30">
            @foreach (var item in Model)
            {
                <div class="col-lg-3 col-md-6 col-sm-6 col-12 mb-30 team-wrapper">
                    <div class="single-team wow fadeIn" data-wow-duration="1.5s" data-wow-delay=".1s">
                        <div class="team-thumb">
                            <a class="image" href="member-details.html"><img src="@item.Image" alt="Team"></a>
                        </div>
                        <ul class="sidebar-social">
                            <li>
                                <a href="@item.Facebook" class="facebook"><i class="fab fa-facebook-f"></i></a>
                            </li>
                            <li>
                                <a href="@item.Twitter" class="twitter"><i class="fab fa-twitter"></i></a>
                            </li>
                            <li>
                                <a href="@item.Linkedl" class="linkedin"><i class="fab fa-linkedin-in"></i></a>
                            </li>
                            <li>
                                <a href="@item.Instagram" class="instagram"><i class="fab fa-instagram"></i></a>
                            </li>
                        </ul>
                        <div class="team-content">
                            <h4 class="title">
                                <a href="member-details.html">@item.NameSurname</a>
                            </h4>
                            <span class="subtitle">@item.Branch</span>
                        </div>
                    </div>
                </div>
            }

        </div>
    </div>
</div>





