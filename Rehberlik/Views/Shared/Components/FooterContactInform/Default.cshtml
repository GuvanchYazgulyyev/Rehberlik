@model IEnumerable<Rehberlik.Models.SqlDat.ContactInformation>



<!-- Footer Widget Start -->
<div class="col-lg-3 col-md-4 col-sm-6 col-12 mb-30 wow fadeIn" data-wow-duration="1.5s" data-wow-delay=".1s">
    @foreach (var item in Model)
    {
        <div class="footer-widget negative-margin">
            <div class="logo">
                <a href="index.html">
                    <img src="~/3/assets/images/logowhite.png" alt="">
                </a>
            </div>
            <ul class="footer-widget-list-icon">
                <li><i class="fas fa-map-marker-alt"></i> <span> @item.Address</span></li>
                <li><i class="fas fa-envelope"></i> <span>@item.Email</span> </li>
                <li><i class="fas fa-phone"></i><span>@item.Tel</span> </li>
            </ul>
            <ul class="social-icon d-flex flex-start">
                <li>
                    <a href="@item.Face" class="facebook"><i class="fab fa-facebook-f"></i></a>
                </li>
                <li>
                    <a href="@item.Twit" class="twitter"><i class="fab fa-twitter"></i></a>
                </li>
                <li>
                    <a href="@item.Linkd" class="linkedin"><i class="fab fa-linkedin-in"></i></a>
                </li>
                <li>
                    <a href="@item.Ins" class="instagram"><i class="fab fa-instagram"></i></a>
                </li>
            </ul>
        </div>
    }
  
</div>
<!-- Footer Widget End -->